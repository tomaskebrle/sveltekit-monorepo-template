{
	"name": "sveltekit-monorepo-template",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"prettier": "prettier --write \"**/*.{ts,tsx,md,svelte}\"",
		"prettier-check": "prettier --plugin-search-dir . --check ."
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier": "latest",
		"turbo": "latest"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"dependencies": {},
	"packageManager": "pnpm@7.22.0"
}
